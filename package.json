{
  "name": "querschnitts_editor",
  "version": "1.0.0",
  "description": "Ermöglicht das Editieren von Querschnitte im WebEditor. Als Datenquelle \r wird hierfür der PublicWFS der TTSIB5 von NOVASIB verwendet. Der \r Editor ist nur in Zusammenhang mit dieser Software einsetzbar.",
  "main": "src/js/edit.ts",
  "scripts": {
    "test": "mocha",
    "coverage": "nyc mocha",
    "startServer": "parcel watch --out-dir \"//gv-srv-w00118/GlassFish Docroot/querOO\" --hmr-hostname localhost --public-url . src/*.html",
    "buildServer": "parcel build --out-dir \"//gv-srv-w00118/GlassFish Docroot/querschnitt\" --public-url . src/*.html",
    "buildBeta": "parcel build --out-dir \"//gv-srv-w00118/GlassFish Docroot/querBeta\" --public-url . src/*.html",
    "start": "parcel src/*.html",
    "build": "parcel build --public-url . src/*.html",
    "validate-typescript": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FlorianTimm/QuerschnittsEditor.git"
  },
  "keywords": [],
  "author": "Florian Timm",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/FlorianTimm/QuerschnittsEditor/issues"
  },
  "browserslist": [
    "> 1%",
    "last 3 versions"
  ],
  "homepage": "https://github.com/FlorianTimm/QuerschnittsEditor#readme",
  "dependencies": {
    "@types/chosen-js": "^1.8.1",
    "@types/jquery": "^3.3.31",
    "@types/jqueryui": "^1.12.9",
    "@types/node": "^12.12.9",
    "@types/ol": "^5.3.6",
    "@types/proj4": "^2.5.0",
    "babel-polyfill": "^6.26.0",
    "chosen-js": "^1.8.7",
    "core-js": "^3.4.1",
    "jquery": "^3.4.1",
    "jquery-ui": "^1.12.1",
    "jquery-ui-bundle": "^1.12.1-migrate",
    "ol": "^6.1.1",
    "proj4": "^2.6.0",
    "regenerator-runtime": "^0.13.2",
    "src": "^1.1.2",
    "ts": "^0.2.2"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^0.1.3",
    "@types/chai": "^4.2.5",
    "@types/mocha": "^5.2.7",
    "chai": "^4.2.0",
    "cssnano": "^4.1.10",
    "source-map-support": "^0.5.16",
    "ts-node": "^8.5.2",
    "typescript": "^3.7.2"
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "extension": [
      ".ts",
      ".tsx"
    ],
    "include": [
      "src/ts/**"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "reporter": [
      "text"
    ],
    "all": true
  }
