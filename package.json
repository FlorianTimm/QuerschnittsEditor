    "buildAlpha": "webpack --output-path \"X:\\\\querOO\" --mode development",
    "buildBeta": "webpack --output-path \"X:\\\\querBeta\" --mode production",
    "buildServer": "webpack --output-path \"X:\\\\querschnitt\" --mode production",
    "validate-typescript": "tsc --noEmit",
    "buildDoc": "npx typedoc"
  },
